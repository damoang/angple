    },
    "keywords": [
        "angple",
        "theme",
        "engine",
        "component",
        "svelte"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@angple/types": "workspace:*",
        "@angple/hook-system": "workspace:*"
    },
    "devDependencies": {
        "typescript": "^5.9.3",
        "vitest": "^3.2.3"
    }
}
