{
    "name": "@angple/theme-engine",
    "version": "0.1.0",
    "description": "Dynamic theme loading and component management for Angple",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
