{
    "id": "damoang-default",
    "name": "Damoang Default",
    "version": "1.0.0",
    "author": {
        "name": "SDK Labs",
        "email": "team@damoang.net"
    },
    "description": "다모앙의 기본 레이아웃을 테마화한 버전입니다. 좌측 사이드바, 우측 패널, 윙 배너를 포함한 클래식한 커뮤니티 레이아웃입니다.",
    "screenshot": "screenshot.png",
    "angpleVersion": "0.1.0",
    "tags": ["default", "classic", "community", "sidebar", "panel"],
    "settings": {
        "layout": {
            "sidebarPosition": {
                "label": "Sidebar Position",
                "type": "select",
                "default": "left",
                "description": "사이드바 위치 설정",
                "options": [
                    {
                        "label": "Left (왼쪽)",
                        "value": "left"
                    },
                    {
                        "label": "Right (오른쪽)",
                        "value": "right"
                    }
                ]
            },
            "showWingBanners": {
                "label": "Show Wing Banners",
                "type": "boolean",
                "default": true,
                "description": "좌우 윙 배너 표시 여부"
            },
            "showPodcastPlayer": {
                "label": "Show Podcast Player",
                "type": "boolean",
                "default": true,
                "description": "팟캐스트 플레이어 표시 여부"
            }
        }
    },
    "components": [],
    "hooks": [],
    "category": "theme",
    "license": "MIT",
    "main": "./dist/index.js"
}
